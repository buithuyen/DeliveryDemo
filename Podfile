# platform :ios, '10.0'

target 'Delivery' do
    # ignore all warnings from all pods
    inhibit_all_warnings!
    
    use_frameworks!

  # Network
  pod 'Moya/RxSwift'
  pod 'ReachabilitySwift'

  # Rx
  pod 'RxSwift'
  pod 'RxCocoa'
  pod 'NSObject+Rx'
  pod 'RxCoreLocation'

  # Map
  pod 'GoogleMaps'

end
